        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title"><%= title %></h3>
            </div>
            <div class="modal-body">
                <div class="content-detail">
                    <ul class="list-group content-ul">
                        <% gonggaoinfos.forEach(function (gonggaoinfo, index) { %>
                            <li>
                                <a href="/XTpost/<%= gonggaoinfo._id%>" class="linkshowmodal" target="_blank"><%= gonggaoinfo.gonggaotitle %></a>
                                <span class="date-annuce pull-right"><%= gonggaoinfo.time.day %></span>
                            </li>
                        <% }) %>
                    </ul>
                    <%- include paging %>
                </div>
            </div>
        </div>
    </div>
</div>
